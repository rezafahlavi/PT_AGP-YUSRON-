     bahan_keluar.`id_bahan_keluar` AS bahan_keluar_id_bahan_keluar,
     bahan_keluar.`id_bahan` AS bahan_keluar_id_bahan,
     bahan_keluar.`nama_bahan` AS bahan_keluar_nama_bahan,
